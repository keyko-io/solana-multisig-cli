{
  "name": "sol-multisig-cli",
  "author": {
    "name": "keyko"
  },
  "description": "CLI to manage Solana transactions using multisig.",
  "version": "0.0.1",
  "main": "lib/index.js",
  "bin": {
    "sol-multisig": "./bin/run"
  },
  "bugs": "https://github.com/keyko-io/solana-multisig-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@project-serum/anchor": "^0.14.0",
    "@project-serum/borsh": "^0.1.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@project-serum/sol-wallet-adapter": "^0.2.6",
    "@saberhq/stableswap-sdk": "^1.10.5",
    "@solana/spl-token-registry": "0.2.819",
    "@solana/web3.js": "^1.31.0",
    "@types/big.js": "^6.1.2",
    "atob": "",
    "bip39": "",
    "bn.js": "^5.2.0",
    "bs58": "",
    "buffer": "6.0.3",
    "buffer-layout": "^1.2.2",
    "ed25519-hd-key": "^1.2.0",
    "hex-to-32": "",
    "inquirer": "^8.2.0",
    "js-base64": "^3.6.0",
    "jsbi": "^4.1.0",
    "tslib": "^1",
    "types-bn": "^0.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^7.0.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "mocha": "^9.1.3",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/keyko-io/solana-multisig-cli",
  "keywords": [
    "multisig",
    "token",
    "saber",
    "solana"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sol-multisig",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "keyko-io/solana-multisig-cli"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "tsc": "tsc -b ."
  }
}
